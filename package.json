{
  "name": "vite-ssr",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "NODE_ENV=development TS_NODE_PROJECT=tsconfig.server.json tsnd --respawn --transpile-only -r tsconfig-paths/register server/main",
    "build": "pnpm clean && pnpm build:client & pnpm build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr app/entry-server.ts --outDir dist/server",
    "clean": "rm -rf dist",
    "serve": "NODE_ENV=production TS_NODE_PROJECT=tsconfig.server.json tsnd --transpile-only -r tsconfig-paths/register server/main"
  },
  "author": "Dominic Marx",
  "dependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/server-renderer": "^3.0.11",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "serve-static": "^1.14.1",
    "vite": "^2.3.4",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/node": "^15.6.1",
    "@vue/compiler-sfc": "^3.0.11",
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
