{
  "name": "express-vite-ssr-typescript",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "NODE_ENV=development TS_NODE_PROJECT=tsconfig.server.json tsnd --respawn --transpile-only --watch server -r tsconfig-paths/register server/bin/www",
    "build": "pnpm clean && pnpm build:client & pnpm build:server",
    "build:client": "vite build --ssrManifest --outDir _dist/client",
    "build:server": "vite build --ssr app/entry-server.ts --outDir _dist/server",
    "clean": "rm -rf _dist",
    "serve": "NODE_ENV=production TS_NODE_PROJECT=tsconfig.server.json tsnd --transpile-only -r tsconfig-paths/register server/bin/www"
  },
  "author": "Dominic Marx",
  "dependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/server-renderer": "^3.0.11",
    "@vueuse/core": "^4.11.1",
    "@vueuse/head": "^0.5.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.0",
    "isomorphic-cookie": "^1.2.4",
    "joi": "^17.4.0",
    "jwt-simple": "^0.5.6",
    "morgan": "^1.10.0",
    "serve-static": "^1.14.1",
    "vite": "^2.3.4",
    "vite-plugin-components": "^0.9.2",
    "vite-plugin-icons": "^0.5.1",
    "vite-plugin-pages": "^0.12.1",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.348",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.6.1",
    "@vue/compiler-sfc": "^3.0.11",
    "nodemon": "^2.0.7",
    "pug": "^3.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "vite-plugin-windicss": "^0.16.0",
    "windicss": "^3.0.12"
  }
}
